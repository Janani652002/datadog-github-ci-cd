{
	"$schema": "https://json.schemastore.org/sarif-2.1.0.json",
	"version": "2.1.0",
	"runs": [
		{
			"tool": {
				"driver": {
					"name": "mytool"
				}
			},
			"results": [
				{
					"ruleId": "my-rule-id",
					"locations": [
						{
							"physicalLocation": {
								"artifactLocation": {
									"uri": "somefile"
								},
								"region": {
									"startLine": 53,
									"endLine": 68,
									"startColumn": 3,
									"endColumn": 14
								}
							}
						}
					],
					"message": {
						"id": "message-id",
						"text": "mytext",
						"markdown": "mdtext"
					}
				}
			]
		}
	]
}
